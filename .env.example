# 1Password vault name for secrets management
# Set this in a local .env file (ignored by git)
# export OP_VAULT="your-vault-name"

# Anthropic API key (https://console.anthropic.com/account/keys)
ANTHROPIC_API_KEY="YOUR_ANTHROPIC_API_KEY"

# OpenAI API key (https://platform.openai.com/account/api-keys)
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"

# Tavily API key (https://app.tavily.com/home)
TAVILY_API_KEY="YOUR_TAVILY_API_KEY"

# LangSmith API key (optional but recommended: https://smith.langchain.com/settings)
LANGSMITH_API_KEY="YOUR_LANGSMITH_API_KEY"

# Enable LangSmith tracing (true/false)
LANGSMITH_TRACING="true"

# LangSmith project name
LANGSMITH_PROJECT="deepagents-research"
